{
  "name": "fioridevbot",
  "version": "0.0.1",
  "description": "Sample from https://docs.botframework.com/en-us/node/builder/guides/understanding-natural-language/#navtitle",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/jfseb/fdevstart.git"
  },
  "bugs": {
    "url": "http://github.com/jfseb/fdevstart/issues"
  },
  "scripts": {
    "setup": "typings install debug && gulp default",
    "test": "gulp test",
    "testOld": "./node_modules/.bin/nodeunit test/match test/utils test/covleven.js && standard",
    "startConsole": "node console_app.js",
    "run": "nodeunit",
    "start": "electron .",
    "ui": "electron .",
    "instrument": "gulp instrument",
    "testcov": "set DO_COVERAGE=1 && nodeunit test/match test/utils test/covleven.js --reporter=lcov >info.lcov",
    "testcov2": "env DO_COVERAGE=1 && nodeunit test/match test/utils test/covleven.js --reporter=lcov | coveralls",
    "testcov3": "env DO_COVERAGE=1 nodeunit test/match test/utils test/covleven.js --reporter=lcov | coveralls",
    "testcov4": "export DO_COVERAGE=1 && printenv && nodeunit test/match test/utils test/covleven.js --reporter=lcov | coveralls",
    "testcov5": "export DO_COVERAGE=1 && nodeunit test/match test/utils test/covleven.js --reporter=lcov | coveralls",

    "tocoveralls": "cat info.lcov | coveralls",
    "tocoveralls2": "cat info.lcov | ./node_modules/.bin/coveralls",
    "cover2": "nodeunit test/covleven.js --reporter=lcov | csoveralls",
    "coveralls": "./node_modules/.bin/nodeunit test/covleven.js --reporter=lcov | ./node_modules/.bin/coveralls",
    "coveralls2": "jscoverage src && YOURPACKAGE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls"
  },
  "keywords": [
    "bot",
    "luis",
    "electron"
  ],
  "standard": {
    "ignore": [
      "src\\utils\\dam*",
      "gen/",
      "gen_cov/",
      "**/out/",
      "src/ts/**/*.js",
      "src-cov/**/*.js",
      "lib/text*",
      "render"
    ]
  },
  "author": "jfsebpublic@gmail.com",
  "license": "MIT",
  "dependencies": {
    "botbuilder": "^3.4.4",
    "debug": "^2.3.2"
  },
  "bin": {
    "nodeunit": ".nodeunit/bin/nodeunit",
    "coverallst": ".coveralls/bin/coveralls"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "test": "./test",
    "doc": "./doc",
    "man": "./man1"
  },
  "babel": {
    "comments": true
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.15",
    "electron": "^1.4.7",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-coveralls": "^0.1.4",
    "gulp-env": "^0.4.0",
    "gulp-instrument": "^0.1.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-log-capture": "0.0.6",
    "gulp-newer": "^1.3.0",
    "gulp-nodeunit": "0.0.5",
    "gulp-sourcemap": "^1.0.1",
    "gulp-standard": "^8.0.2",
    "gulp-typescript": "^3.1.3",
    "gulp-watch": "^4.3.11",
    "jscoverage": "^0.6.0",
    "nodeunit": "^0.10.2",
    "request-defaults": "^1.2.0",
    "standard": "^8.5.0",
    "textarea-autogrow": "^1.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^2.0.10"
  }
}
