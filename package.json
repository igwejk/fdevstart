{
  "name": "fioridevbot",
  "version": "0.0.1",
  "description": "Sample from https://docs.botframework.com/en-us/node/builder/guides/understanding-natural-language/#navtitle",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/jfseb/fdevstart.git"
  },
  "bugs": {
    "url": "http://github.com/jfseb/fdevstart/issues"
  },
  "scripts": {
    "setup": "typings install debug && gulp default",
    "test": "gulp test",
    "testOld": "./node_modules/.bin/nodeunit test/match test/utils && eslint",
    "startConsole": "node console_app.js",
    "run": "nodeunit",
    "start": "node server",
    "startui": "electron .",
    "ui": "electron .",
    "instrument": "jscoverage gen2/ gen_cov/",
    "instrumentGULP": "gulp instrument",
    "testcovWIN": "set FSD_COVERAGE=1 && nodeunit test/match test/exec test/model test/bot test/utils test/graph --reporter=lcov >info.lcov",
    "testcovNoReporter": "export FSD_COVERAGE=1 && nodeunit test/match test/exec test/model test/bot test/utils test/graph ",
    "testcovNoPipe": "export FSD_COVERAGE=1 && nodeunit test/match test/exec test/bot test/utils test/graph --reporter=lcov",
    "testcov": "export FSD_COVERAGE=1 && nodeunit test/match test/exec test/bot test/model test/utils test/graph --reporter=lcov | coveralls",
    "tocoveralls": "cat info.lcov | coveralls",
    "tocoveralls2": "cat info.lcov | ./node_modules/.bin/coveralls",
    "coveralls2": "jscoverage src && YOURPACKAGE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls"
  },
  "keywords": [
    "bot",
    "luis",
    "electron"
  ],
  "standard": {
    "ignore": [
      "src\\utils\\dam*",
      "gen/",
      "gen_cov/",
      "**/out/",
      "src/ts/**/*.js",
      "src-cov/**/*.js",
      "lib/text*",
      "render"
    ]
  },
  "author": "jfsebpublic@gmail.com",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "botbuilder": "^3.4.4",
    "connect-pg-simple": "^3.1.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.3.2",
    "elizabot": "0.0.2",
    "emailjs": "^1.0.8",
    "errorhandler": "^1.5.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "express-socket.io-session": "^1.3.1",
    "jade": "^1.11.0",
    "level": "^1.5.0",
    "levelgraph": "^1.3.1",
    "pg": "^6.1.2",
    "socket.io": "^1.7.2",
    "stream-to-array": "^2.3.0",
    "stylus": "^0.54.5"
  },
  "bin": {
    "nodeunit": ".nodeunit/bin/nodeunit",
    "coverallst": ".coveralls/bin/coveralls"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "test": "./test",
    "doc": "./doc",
    "man": "./man1"
  },
  "babel": {
    "comments": true
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/pg": "^6.1.34",
    "@types/core-js": "^0.9.35",
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.15",
    "debug-levels": "^0.2.0",
    "electron": "^1.4.7",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-coveralls": "^0.1.4",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-instrument": "^0.1.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-log-capture": "0.0.6",
    "gulp-newer": "^1.3.0",
    "gulp-nodeunit": "0.0.5",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemap": "^1.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-watch": "^4.3.11",
    "jscoverage": "^0.6.0",
    "levelgraph": "^1.3.1",
    "node-uuid": "^1.4.7",
    "nodeunit": "^0.10.2",
    "request-defaults": "^1.2.0",
    "socket.io": "^1.7.1",
    "stream-to-array": "^2.3.0",
    "textarea-autogrow": "^1.0.0",
    "typescript": "^2.0.10"
  }
}
