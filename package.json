{
  "name": "fioridevbot",
  "version": "0.0.1",
  "description": "Sample from https://docs.botframework.com/en-us/node/builder/guides/understanding-natural-language/#navtitle",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/jfseb/fdevstart.git"
  },
  "bugs": {
    "url": "http://github.com/jfseb/fdevstart/issues"
  },
  "scripts": {
    "test2": "node console_app.js",
    "run": "nodeunit",
    "start": "electron .",
    "startN": "node app3.js",
    "ui": "electron .",
    "instrument": "gulp instrument",
    "testcover": "nodeunit test/covleven.js --reporter=lcov >info.lcov",
    "tocoveralls": "cat info.lcov | coveralls",
    "test": "./node_modules/.bin/nodeunit test/match test/utils test/covleven.js && standard",
    "cover2": "nodeunit test/covleven.js --reporter=lcov | csoveralls",
    "coveralls": "./node_modules/.bin/nodeunit test/covleven.js --reporter=lcov | ./node_modules/.bin/coveralls",
    "coveralls2": "jscoverage src && YOURPACKAGE_COVERAGE=1 nodeunit --reporter=lcov test | coveralls"
  },
  "keywords": [
    "bot",
    "luis",
    "electron"
  ],
  "standard": {
    "ignore": [
      "src\\utils\\dam*",
      "**/out/",
      "src/ts/**/*.js",
      "src-cov/**/*.js",
      "lib/text*",
      "render"
    ]
  },
  "author": "jfsebpublic@gmail.com",
  "license": "MIT",
  "dependencies": {
    "botbuilder": "^3.4.4",
    "debug": "^2.3.2"
  },
  "bin": {
    "nodeunit": ".nodeunit/bin/nodeunit",
    "coverallst": ".coveralls/bin/coveralls"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "test": "./test",
    "doc": "./doc",
    "man": "./man1"
  },
  "devDependencies": {
    "electron": "^1.4.7",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "grunt": "^1.0.1",
    "grunt-contrib-nodeunit": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-standard": "^2.0.0",
    "grunt-tsc": "^1.0.12",
    "gruntify-eslint": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-coverage": "^0.3.38",
    "gulp-coveralls": "^0.1.4",
    "gulp-instrument": "^0.1.0",
    "gulp-jscoverage": "^0.1.0",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-log-capture": "0.0.6",
    "gulp-nodeunit": "0.0.5",
    "gulp-sourcemap": "^1.0.1",
    "gulp-standard": "^8.0.2",
    "gulp-typescript": "^3.1.3",
    "jscoverage": "^0.6.0",
    "nodeunit": "^0.10.2",
    "request-defaults": "^1.2.0",
    "standard": "^8.5.0",
    "textarea-autogrow": "^1.0.0",
    "typescript": "^2.0.10"
  }
}
